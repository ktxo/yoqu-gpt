{
  "name": "chatgpt2",
  "desc": "Configuration default 2 - myemail@gmail.com",
  "type": "chatgpt",
  "dump": true,
  "dump_folder": "dump/chatgpt2",
  "resource": {
    "local": false,
    "debug": false,
    "url": "https://chat.openai.com",
    "url_login": "https://openai.com",
    "url_title": "ChatGPT",
    "command": [
      "/opt/google/chrome/chrome",
      "--remote-debugging-port=9223",
      "--user-data-dir=chrome_data/chatgpt2"
    ],
    "chromedriver": "./chromedriver",
    "sleep_range": [1, 3],
    "options": ["--log-level=0", "--disable-logging", "--disable-dev-shm-usage"],
    "options_experimental": [
      ["debuggerAddress", "127.0.0.1:9223"]
    ]
  },
  "wait": 10
}
